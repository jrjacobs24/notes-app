    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "lowdb": "^1.0.0",
    "normalize.css": "^8.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-act": "^1.7.4"
  },
